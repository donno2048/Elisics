      <data name="Stop 1 (s)">t1</data>
      <data name="Stop 2 (s)">t2</data>
      <data name="Stop 3 (s)">t3</data>
      <data name="Stop 4 (s)">t4</data>
      <data name="Stop 5 (s)">t5</data>
    </set>
    <set name="All">
      <data name="Event time (s)">tlist</data>
      <data name="Interval (s)">dtlist</data>
    </set>
  </export>
